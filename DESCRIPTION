Package: DDPM
Type: Package
Title: Data Sets for Discrete Probability Models
Version: 0.1.0
Authors@R: c(person("Christophe", "Chesneau", role = "aut", email = "christophe.chesneau@unicaen.fr"), person( "Muhammad", "Imran", role = c("aut", "cre"), email = "imranshakoor84@yahoo.com" ),
